<xs:complexType name="DecisionDateType">
		<xs:sequence>
			<xs:element ref="base:Date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DecisionDate" type="DecisionDateType">
		<xs:annotation>
			<xs:documentation>决定日期</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="AppealCasePartiesType">
		<xs:sequence>
			<xs:choice>
				<xs:element ref="Complainant" maxOccurs="unbounded"/>
				<xs:element ref="Appellant" maxOccurs="unbounded"/>
			</xs:choice>
