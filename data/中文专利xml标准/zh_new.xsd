		<xs:complexType name="DefinitionDescriptionType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="Bold"/>
			<xs:element ref="Italic"/>
			<xs:element ref="Underline"/>
			<xs:element ref="Overscore"/>
			<xs:element ref="Sup"/>
			<xs:element ref="Sub"/>
			<xs:element ref="SmallCapitals"/>
			<xs:element ref="PageBegin"/>
			<xs:element ref="Break"/>
			<xs:element ref="Preservation"/>
			<xs:element ref="business:ApplicationCitation"/>
			<xs:element ref="business:NPLCitation"/>
			<xs:element ref="business:BiologicalDeposit"/>
			<xs:element ref="CrossReference"/>
			<xs:element ref="FigureReference"/>
			<xs:element ref="Image"/>
			<xs:element ref="UnorderedLists"/>
			<xs:element ref="OrderedLists"/>
			<xs:element ref="Chemistry"/>
			<xs:element ref="MathDetails"/>
			<xs:element ref="business:DecisionVerdictRelated"/>
