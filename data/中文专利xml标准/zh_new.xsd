			<xs:documentation>文档页</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ChemicalPointerType">
		<xs:complexContent>
			<xs:restriction base="xs:anyType">
				<xs:attribute name="id" type="xs:ID"/>
				<xs:attribute name="file" type="xs:string" use="required"/>
				<xs:attribute name="chemType" type="xs:string"/>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ChemicalPointer" type="ChemicalPointerType">
		<xs:annotation>
			<xs:documentation>外部化学式 xml 文件，遵循CML加工标准</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ChemistryType">
		<xs:sequence>
			<xs:element ref="Image" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="FormulaText" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="ChemicalPointer" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="business:STRmol" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:NMTOKEN">
			<xs:annotation>
				<xs:documentation>建议取值为：“文献号+chem+6位流水号"</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="num" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:element name="Chemistry" type="ChemistryType">
		<xs:annotation>
			<xs:documentation>化学公式、方程式、结构式等</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="MathFormulaType">
		<xs:complexContent>
			<xs:restriction base="xs:anyType">
				<xs:attribute name="id" type="xs:ID"/>
				<xs:attribute name="file" type="xs:string" use="required"/>
				<xs:attribute name="mathType" type="xs:string"/>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="MathFormula" type="MathFormulaType">
		<xs:annotation>
			<xs:documentation>外部数学公式 xml 文件，遵循MathML加工标准</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="MathType">
		<xs:group ref="m:MathExpression"/>
	</xs:complexType>
	<xs:element name="Math" type="MathType">
		<xs:annotation>
			<xs:documentation>数学公式，遵循MathML加工标准</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="MathDetailsType">
		<xs:sequence>
			<xs:element ref="Image" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="FormulaText" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Math" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="MathFormula" minOccurs="0" maxOccurs="unbounded"/>
<xs:element ref="MathText" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:NMTOKEN">
			<xs:annotation>
				<xs:documentation>建议取值为：“文献号+math+6位流水号"</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="num" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:element name="MathDetails" type="MathDetailsType">
