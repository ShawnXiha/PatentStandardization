							<xs:documentation>源于检索报告</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="EXA">
						<xs:annotation>
							<xs:documentation>检查阶段</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="APP">
						<xs:annotation>
							<xs:documentation>申请人引用</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="115">
						<xs:annotation>
							<xs:documentation>条款115</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="OPP">
						<xs:annotation>
							<xs:documentation>异议阶段</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="appNumber" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>申请号</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="generateBy" use="optional">
			<xs:annotation>
				<xs:documentation>指明该结果是由人工生成还是机器自动生成</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
