			</xs:sequence>
			<xs:element ref="CorrespondingDocs" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="ClassificationIPC" minOccurs="0"/>
			<xs:element ref="ClassificationIPCRDetails" minOccurs="0"/>
			<xs:element ref="ClassificationNational" minOccurs="0"/>
