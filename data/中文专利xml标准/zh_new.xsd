				<xs:attribute name="page" type="xs:string" use="required"/>
				<xs:attribute name="displayMode" type="xs:string" use="required"/>
				<xs:attribute name="coordinate" type="xs:string" use="required"/>
				<xs:attribute name="action" type="xs:string" use="required"/>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BookMark" type="BookMarkType">
