			<xs:documentation>图像文件信息</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="DocPageType">
		<xs:complexContent>
			<xs:restriction base="xs:anyType">
				<xs:attribute name="id" type="xs:ID"/>
				<xs:attribute name="he" type="xs:string" use="required"/>
				<xs:attribute name="wi" type="xs:string" use="required"/>
				<xs:attribute name="type" use="required">
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="tif"/>
