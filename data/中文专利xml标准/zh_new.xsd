		</xs:annotation>
	</xs:element>
	<xs:complexType name="OrderedListsType">
		<xs:sequence>
			<xs:element ref="ListItem" maxOccurs="unbounded"/>
			<xs:element ref="Paragraphs" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="compact">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="compact"/>
