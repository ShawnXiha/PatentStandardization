			<xs:element ref="ExaminerDetails"/>
			<xs:element ref="PublicationInformation"/>
			<xs:element ref="AccessoryDetails" minOccurs="0"/>
			<xs:element ref="DecisionAbstract"/>
			<xs:element ref="Abstract" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="base:Keyword" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="DecisionDrawings" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AppealCaseInformation" type="AppealCaseInformationType">
