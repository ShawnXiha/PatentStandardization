		<xs:attribute name="id" type="xs:ID">
			<xs:annotation>
				<xs:documentation>标识号</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="classificationKind" type="xs:string">
			<xs:annotation>
				<xs:documentation>分类的种类，取值可以是field（表示范畴分类）、PHC（表示药物范畴分类）、practical（表示实用专利分类）、industrial（表示国民经济行业分类）</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="number" type="xs:string">
			<xs:annotation>
				<xs:documentation>分类号的数量</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="applicationCountry" type="xs:string">
			<xs:annotation>
				<xs:documentation>申请国别,专利申请的国家，参考ST.3</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="authorizationCcountry" type="xs:string">
			<xs:annotation>
				<xs:documentation>授权国别，专利授权的国家，参考ST.3</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type" type="xs:string">
			<xs:annotation>
				<xs:documentation>文献种类，参考ST.16</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="documentStatus" type="xs:string">
			<xs:annotation>
				<xs:documentation>文献状态：申请/公开/授权</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="dataStatus" type="xs:string">
			<xs:annotation>
				<xs:documentation>数据状态：过档/新增/更正/删除</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="createDate" type="xs:string">
			<xs:annotation>
				<xs:documentation>分类号的创建日期</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="creator" type="xs:string">
			<xs:annotation>
				<xs:documentation>分类号的加工方</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
