			<xs:documentation>书签</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PictureType">
		<xs:complexContent>
			<xs:restriction base="xs:anyType">
				<xs:attribute name="pictureName" type="xs:string" use="required"/>
				<xs:attribute name="pictureNumber" type="xs:string" use="required"/>
				<xs:attribute name="pictureType" type="xs:string" use="required"/>
				<xs:attribute name="originName" type="xs:string" use="required"/>
				<xs:attribute name="originPicturePage" type="xs:string" use="required"/>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Picture" type="PictureType">
