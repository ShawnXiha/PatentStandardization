		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="markushList" use="optional">
			<xs:annotation>
				<xs:documentation>取值为True时，标识该列表为一个Markush定义列表</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="true"/>
					<xs:enumeration value="false"/>
				</xs:restriction>
			</xs:simpleType>
