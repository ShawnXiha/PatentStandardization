<?xml version = "1.0" encoding = "UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tbl="http://www.oasis-open.org/tables/exchange/1.0"
           xmlns:m="http://www.w3.org/1998/Math/MathML">
	<xs:import namespace="http://www.oasis-open.org/tables/exchange/1.0" schemaLocation="OASISTable_V1_0.xsd"/>
	<xs:import namespace="http://www.w3.org/1998/Math/MathML" schemaLocation="FlattenedMathML3.xsd"/>
	<xs:element name="Date" type="xs:string">
		<xs:annotation>
			<xs:documentation>日期</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ActionDate" type="DateType">
		<xs:annotation>
			<xs:documentation>生效日期</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="DateType" mixed="true">
		<xs:sequence>
			<xs:element ref="Date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Time" type="xs:time">
		<xs:annotation>
			<xs:documentation>时间</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Birthday" type="DateType">
		<xs:annotation>
			<xs:documentation>生日</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="DateType" mixed="true">
		<xs:sequence>
			<xs:element ref="Date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="OpenDate" type="DateType">
		<xs:annotation>
			<xs:documentation>公开日</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="DateType" mixed="true">
		<xs:sequence>
			<xs:element ref="Date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressBookType">
		<xs:choice>
			<xs:sequence>
				<xs:sequence>
					<xs:choice>
						<xs:element ref="Name" minOccurs="0"/>
						<xs:sequence>
							<xs:element ref="Prefix" minOccurs="0"/>
							<xs:choice>
								<xs:element ref="LastName"/>
								<xs:element ref="OrganizationName"/>
							</xs:choice>
							<xs:element ref="FirstName" minOccurs="0"/>
							<xs:element ref="MiddleName" minOccurs="0"/>
							<xs:element ref="FirstLastName" minOccurs="0"/>
							<xs:element ref="SecondLastName" minOccurs="0"/>
							<xs:element ref="Suffix" minOccurs="0"/>
							<xs:element ref="IndividualID" minOccurs="0"/>
							<xs:element ref="Role" minOccurs="0"/>
							<xs:element ref="OrganizationName" minOccurs="0"/>
							<xs:element ref="Department" minOccurs="0"/>
							<xs:element ref="SynonymicalName" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:choice>
					<xs:element ref="RegisteredNumber" minOccurs="0"/>
				</xs:sequence>
				<xs:element ref="Address" minOccurs="0"/>
				<xs:element ref="Phone" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Fax" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Email" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="URL" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="EAddress" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="DescriptiveText" minOccurs="0"/>
			</xs:sequence>
			<xs:element ref="Text"/>
		</xs:choice>
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="lang" type="xs:string"/>
	</xs:complexType>
	<xs:element name="AddressBook" type="AddressBookType">
		<xs:annotation>
			<xs:documentation>地址薄</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EAddress" type="BaseType">
		<xs:annotation>
			<xs:documentation>电子地址</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:complexType name="EmailType" mixed="true">
		<xs:complexContent mixed="true">
			<xs:extension base="BaseType">
				<xs:attribute name="emailPurpose">
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="informal"/>
							<xs:enumeration value="main"/>
							<xs:enumeration value="advance"/>
							<xs:enumeration value="general"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Email" type="EmailType">
		<xs:annotation>
			<xs:documentation>电子邮件</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="Fax" type="BaseType">
		<xs:annotation>
			<xs:documentation>传真号码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="Phone" type="BaseType">
		<xs:annotation>
			<xs:documentation>电话号码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="URL" type="BaseType">
		<xs:annotation>
			<xs:documentation>统一资源定位</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="AddressType">
		<xs:sequence>
			<xs:element ref="AddressLine" minOccurs="1"/>
			<xs:element ref="AddressMailCode" minOccurs="1"/>
			<xs:element ref="PostBox" minOccurs="1"/>
			<xs:element ref="AddressRoom" minOccurs="1"/>
			<xs:element ref="AddressFloor" minOccurs="1"/>
			<xs:element ref="AddressBuilding" minOccurs="1"/>
			<xs:element ref="Street" minOccurs="1"/>
			<xs:element ref="AddressCity" minOccurs="1"/>
			<xs:element ref="County" minOccurs="1"/>
			<xs:choice>
				<xs:element ref="State" minOccurs="1"/>
				<xs:element ref="Province" minOccurs="1"/>
			</xs:choice>
			<xs:element ref="PostCode" minOccurs="1"/>
			<xs:element ref="WIPOST3Code" minOccurs="1" maxOccurs="unbounded"/>
			<xs:element ref="Text" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Address" type="AddressType">
		<xs:annotation>
			<xs:documentation>地址</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="AddressLine" type="BaseType">
		<xs:annotation>
			<xs:documentation>地址行</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="AddressFloor" type="BaseType">
		<xs:annotation>
			<xs:documentation>楼层信息</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="AddressBuilding" type="BaseType">
			<xs:annotation>
				<xs:documentation>建筑物信息</xs:documentation>
			</xs:annotation>
		</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="AddressCity" type="BaseType">
		<xs:annotation>
			<xs:documentation>城、镇</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="County" type="BaseType">
		<xs:annotation>
			<xs:documentation>县</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="WIPOST3Code" type="BaseType">
		<xs:annotation>
			<xs:documentation>国别</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="AddressMailCode" type="BaseType">
		<xs:annotation>
			<xs:documentation>邮局编码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="NationalityType">
		<xs:sequence>
			<xs:element ref="WIPOST3Code"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Nationality" type="NationalityType">
		<xs:annotation>
			<xs:documentation>国籍</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="PostBox" type="BaseType">
		<xs:annotation>
			<xs:documentation>邮箱号码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="PostCode" type="BaseType">
		<xs:annotation>
			<xs:documentation>邮政编码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="RegionType">
		<xs:sequence>
			<xs:element ref="WIPOST3Code"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Region" type="RegionType">
		<xs:annotation>
			<xs:documentation>地区、国家</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ResidenceType">
		<xs:sequence>
			<xs:element ref="WIPOST3Code"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Residence" type="ResidenceType">
		<xs:annotation>
			<xs:documentation>居住国</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="AddressRoom" type="BaseType">
		<xs:annotation>
			<xs:documentation>房间号码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="State" type="BaseType">
		<xs:annotation>
			<xs:documentation>地区、州、省</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="Street" type="BaseType">
		<xs:annotation>
			<xs:documentation>街道</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="NationalType">
		<xs:sequence maxOccurs="unbounded">
			<xs:element ref="WIPOST3Code"/>
			<xs:element ref="business:ProtectionRequest" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="National" type="NationalType">
		<xs:annotation>
			<xs:documentation>国家信息</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="RegionalType">
		<xs:sequence>
			<xs:element ref="Region"/>
			<xs:sequence maxOccurs="unbounded">
				<xs:element ref="WIPOST3Code"/>
				<xs:element ref="business:ProtectionRequest" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:element ref="business:AnyOtherState" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Regional" type="RegionalType">
		<xs:annotation>
			<xs:documentation>保护区域</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ProvinceType">
		<xs:complexContent>
			<xs:extension base="BaseType">
				<xs:attribute name="description" use="optional">
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="code">
								<xs:annotation>
									<xs:documentation>代码表示</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="word">
								<xs:annotation>
									<xs:documentation>自然语言描述</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Province" type="ProvinceType">
		<xs:annotation>
			<xs:documentation>省份</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="FirstLastName" type="BaseType">
		<xs:annotation>
			<xs:documentation>父亲的姓</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="FirstName" type="BaseType">
		<xs:annotation>
			<xs:documentation>名</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="LastName" type="BaseType">
		<xs:annotation>
			<xs:documentation>姓</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="MiddleName" type="BaseType">
		<xs:annotation>
			<xs:documentation>中间名</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="NameType" mixed="true">
		<xs:complexContent mixed="true">
			<xs:extension base="BaseType">
				<xs:attribute name="nameType">
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="natural">
								<xs:annotation>
									<xs:documentation>自然人</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
							<xs:enumeration value="legal">
								<xs:annotation>
									<xs:documentation>法人</xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Name" type="NameType">
		<xs:annotation>
			<xs:documentation>名称</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="Prefix" type="BaseType">
		<xs:annotation>
			<xs:documentation>称谓</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="SecondLastName" type="BaseType">
		<xs:annotation>
			<xs:documentation>母亲的姓</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="BaseType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
	</xs:complexType>
	<xs:element name="Suffix" type="BaseType">
		<xs:annotation>
			<xs:documentation>名字中的后缀</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="SynonymicalName" type="BaseType">
		<xs:annotation>
			<xs:documentation>别名</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="OrganizationName" type="BaseType">
		<xs:annotation>
			<xs:documentation>机构名称</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="IndividualID" type="BaseType">
		<xs:annotation>
			<xs:documentation>个体代码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Role" type="BaseType">
		<xs:annotation>
			<xs:documentation>职位/角色</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="Department" type="BaseType">
		<xs:annotation>
			<xs:documentation>部门</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="RegisteredNumber" type="BaseType">
		<xs:annotation>
			<xs:documentation>注册号码</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="TableFileType">
			<xs:complexContent>
				<xs:restriction base="xs:anyType">
					<xs:attribute name="id" type="xs:NMTOKEN">
						<xs:annotation>
							<xs:documentation>建议取值为：“文献号+tabl+6位流水号"</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="file" type="xs:string" use="optional"/>
					<xs:attribute name="tableType" type="xs:string"/>
					<xs:attribute name="relativeLocation" type="xs:string">
						<xs:annotation>
							<xs:documentation>相对位置，表示为“pp;Tab.n;row;col"
						   其中，pp为页码；Tab.n为图号；row为行号；col为列号
						   </xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="coordinate" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>
						   坐标，表示为“nnnn;x,xl;y,yl"形式。
						   其中：
						   nnnn为页码信息；
						   x、y分别为x轴、y轴的坐标起点位置；
						   xl、yl分别为x轴、y轴的长度
						   </xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
	<xs:element name="TableFile" type="TableFileType">
		<xs:annotation>
			<xs:documentation>表格文件</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="TableType">
		<xs:sequence>
			<xs:element ref="Title"/>
			<xs:element ref="tbl:tgroup"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:ID"/>
	</xs:complexType>
	<xs:element name="Table" type="TableType">
		<xs:annotation>
			<xs:documentation>表格，遵循OASIS标准</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="TableExternalDocType" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		<xs:attribute name="id" type="xs:ID"/>
		<xs:attribute name="file" type="xs:string" use="required"/>
		<xs:attribute name="lang" type="xs:string"/>
		<xs:attribute name="externalType">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="chemistry"/>
					<xs:enumeration value="programListing"/>
					<xs:enumeration value="math"/>
					<xs:enumeration value="other"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="status" type="xs:string"/>
		<xs:attribute name="carriers">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="paper"/>
					<xs:enumeration value="internalElectronic"/>
					<xs:enumeration value="externalElectronic"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="TableExternalDoc" type="TableExternalDocType">
		<xs:annotation>
			<xs:documentation>外部独立表格文档</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="TableDetailsType">
		<xs:sequence>
			<xs:element ref="Image" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Table" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="TableFile" minOccurs="0" maxOccurs="unbounded"/>
<xs:element ref="TableText" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:NMTOKEN">
			<xs:annotation>
				<xs:documentation>建议取值为：“文献号+tabl+6位流水号"</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="num" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:element name="TableDetails" type="TableDetailsType">
		<xs:annotation>
			<xs:documentation>表格信息</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TableText" type="TextType">
		<xs:annotation>
			<xs:documentation>存放以CDATA的方式存储、且不满足OASIS标准的表格数据</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ImageTextType">
		<xs:sequence>
			<xs:element ref="Paragraphs"/>
		</xs:sequence>
		<xs:attribute name="sequence" type="xs:string">
			<xs:annotation>
				<xs:documentation>序列号，取值为自然数</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="imageTextType">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="imageCaption">
						<xs:annotation>
							<xs:documentation>图像标题</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="reference sign">
						<xs:annotation>
							<xs:documentation>附图标记</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="reference sign coordinates">
						<xs:annotation>
							<xs:documentation>附图标记的起始坐标</xs:documentation>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="ImageText" type="ImageTextType">
		<xs:annotation>
			<xs:documentation>图象的代码化内容</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FigureType">
			<xs:sequence>
				<xs:element ref="Image"/>
				<xs:element ref="DrawingText" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="id" type="xs:ID"/>
			<xs:attribute name="num" type="xs:string" use="required"/>
			<xs:attribute name="figureLabels" type="xs:string"/>
			<xs:attribute name="pageEnd" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>仅当当前图位于某一页的结束处时使用该属性，属性值为该图所结束的页码号</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	<xs:element name="Figure" type="FigureType">
		<xs:annotation>
			<xs:documentation>一页图形/图像</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="ImageType">
		<xs:sequence>
			<xs:element ref="ImageText" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:NMTOKEN">
			<xs:annotation>
				<xs:documentation>建议取值为：“文献号+4位类型名称+6位流水号"</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="he" type="xs:NMTOKEN" use="optional"/>
		<xs:attribute name="wi" type="xs:NMTOKEN" use="optional"/>
		<xs:attribute name="top" type="xs:NMTOKEN"/>
		<xs:attribute name="left" type="xs:NMTOKEN"/>
		<xs:attribute name="file" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>若图像和PDF在同一文件夹下，则仅指文件名；反之若不在同一文件夹下，需要指明绝对路径和文件名</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="alt" type="xs:string"/>
		<xs:attribute name="imgContent" default="drawing">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="photograph"/>
					<xs:enumeration value="dna"/>
					<xs:enumeration value="drawing"/>
					<xs:enumeration value="character"/>
					<xs:enumeration value="undefined"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="imgFormat" default="TIFF">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:annotation>
						<xs:documentation>
				       黑白电子图像推荐采用TIFF格式或8位JPEG格式。
                       灰度电子图像推荐采用8位JPEG格式。
                       彩色电子图像推荐采用24位JPEG格式</xs:documentation>
					</xs:annotation>
					<xs:enumeration value="TIFF"/>
					<xs:enumeration value="JPEG"/>
					<xs:enumeration value="PDF"/>
					<xs:enumeration value="st33"/>
					<xs:enumeration value="st35"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="resolution" type="xs:string">
			<xs:annotation>
				<xs:documentation>图像分辨率，分辨率取值应在150DPI和400DPI之间，推荐分辨率300DPI</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="orientation" default="portrait">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="landscape"/>
					<xs:enumeration value="portrait"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="inline" default="no">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="yes"/>
					<xs:enumeration value="no"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="embedded" use="optional">
			<xs:annotation>
				<xs:documentation>该图像是否是嵌入于文档中的，是的话取值为"true"，否则为"false"</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="true"/>
					<xs:enumeration value="false"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="Image" type="ImageType">
		<xs:annotation>
			<xs:documentation>图像数据</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="DrawingTextType">
		<xs:sequence>
			<xs:element ref="Paragraphs"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DrawingText" type="DrawingTextType">
		<xs:annotation>
			<xs:documentation>图像中文字的代码化内容</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="FiguresPhotoesType">
		<xs:sequence>
			<xs:element ref="business:Drawings"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="FiguresPhotoes" type="FiguresPhotoesType">
		<xs:annotation>
			<xs:documentation>图像，照片</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PDFImageFileType">
