									<xsd:element ref="com:ApplicationNumber" minOccurs="0"/>
									<xsd:element ref="com:PriorityApplicationFilingDate"/>
								</xsd:sequence>
		</xsd:complexType>
	
	<xsd:element name="GIPublication" type="gin:GIPublicationType">
		<xsd:annotation>
			<xsd:documentation>Data regarding one geographical indication publication</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="GIPublicationBag" type="gin:GIPublicationBagType">
