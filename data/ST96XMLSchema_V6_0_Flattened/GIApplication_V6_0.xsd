					<xsd:documentation>Manufactured products</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Natural goods">
				<xsd:annotation>
					<xsd:documentation>Natural goods</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Other">
				<xsd:annotation>
					<xsd:documentation>Other</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	
<xsd:element name="ProductChemicalCharacteristicText" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Chemical characteristics of the product</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
	
	<xsd:element name="ProductIndicationBag" type="gin:ProductIndicationBagType">
		<xsd:annotation>
			<xsd:documentation>Collection of information related to the indication of goods and services protected under the geographical indication</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:complexType name="ProductIndicationBagType">
		<xsd:sequence>
			<xsd:element ref="tmk:GoodsServices" maxOccurs="unbounded"/>
			<xsd:element ref="gin:ProductCategory" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:attribute name="productionAmountUnitCategory" type="gin:ProductionAmountUnitCategoryType">
		<xsd:annotation>
			<xsd:documentation>units of the production amount</xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	
	<xsd:simpleType name="ProductionAmountUnitCategoryType">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="Litre">
				<xsd:annotation>
					<xsd:documentation>Litre, equal to 1000 cubic centimetres</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Tonne">
				<xsd:annotation>
					<xsd:documentation>Metric tonne, equal to 1000 kilograms</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	
<xsd:element name="ProductionDescriptionText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Description of production or processing of the product</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="ProductionMaximumAmount" type="gin:ProductionMaximumAmountType">
		<xsd:annotation>
			<xsd:documentation>Maximum yield / amount produced and unit</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:complexType name="ProductionMaximumAmountType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:token">
				<xsd:attribute ref="gin:productionAmountUnitCategory"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	
<xsd:element name="ProductLabelingRuleText" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Specific rules concerning labelling of the product the registered name refers to </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
	
<xsd:element name="ProductMaterialText" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Feed and raw materials of the product</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
	
	<xsd:element name="ProductMaximumAcidity" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Maximum acidity</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="ProductMaximumAlcoholStrength" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Maximum alcohol strength</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="ProductMinimumAcidity" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Minimum acidity</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="ProductMinimumAlcoholStrength" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Minimum alcohol strength</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="ProductOenologicalPracticeText" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Oenological practice and type</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
<xsd:element name="ProductOrganolepticCharacteristicText" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Organoleptic characteristics of the product</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
	
<xsd:element name="ProductPhysicalCharacteristicText" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Physical characteristics of the product</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
	
<xsd:element name="ProductProcessingRuleText" type="xsd:string">
	<xsd:annotation>
		<xsd:documentation>Specific rules concerning slicing, grating, packaging</xsd:documentation>
	</xsd:annotation>
</xsd:element>
	
	<xsd:element name="ProductSpecificationPublication" type="gin:GIPublicationType">
		<xsd:annotation>
			<xsd:documentation>Reference to publication of the product specification</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="ProductSpiritRequirementText" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Spirit requirements</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
<xsd:element name="ProductTerritoryLinkDescriptionText" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Description of the link between the product or service and the geographic region of production or processing of the product</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
	
<xsd:element name="PublicationAmendmentReasonText" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Reason of the amendment or change in a publication</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="PublicationEndPageText" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Identification or number of the last page of publication</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="PublicationStartPageText" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Identification or number of the first page of publication</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="QualityFactorText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Geographic area quality factor</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="QualityProduct" type="gin:QualityProductType">
		<xsd:annotation>
			<xsd:documentation>Quality products information</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="QualityProductNameText" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Quality product type</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="QualityProductPropertyText" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Quality product properties</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:complexType name="QualityProductType">
		<xsd:sequence>
			<xsd:element ref="gin:QualityProductNameText"/>
			<xsd:element ref="gin:QualityProductPropertyText"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="RawMaterialDescriptionText" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Description of raw materials (for processed products only)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="RawMaterialOnlySourceIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Raw material entirely sourced in the geographical area indicator</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="RawMaterialOriginRestrictionIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Restriction of origin of the raw materials indicator</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="RawMaterialQualityRequirementIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Quality required applies to the raw materials indicator</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="ReputationFactorText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Geographic area reputation factor</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="ReservationNameText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Reservation name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="SameCategoryProductSpecificCharacteristicText" type="xsd:token">
		<xsd:annotation>
			<xsd:documentation>Specific characteristics of spirit drinks of the same category</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="SpecificCharacterKeyElementText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Description of key elements establishing the product’s specific character, for a TSG claim</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="SpecificLabelingRuleIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Any specific rules on labelling indicator</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="SpecificProcessingRuleIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indicates whether there are specific rules on slicing, bottling, grating or packaging</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="TraditionalCharacterKeyElementText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Description of key elements establishing the product’s traditional character, for a TSG claim</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="TraditionalUseDescriptionText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Explanation of the traditional use</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="TraditionalUseIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indicates that this geographical indication has a traditional use</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="TSGClaim" type="gin:TSGClaimType">
		<xsd:annotation>
			<xsd:documentation>Claim(s) made for traditional specialities guaranteed</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:complexType name="TSGClaimType">
		<xsd:sequence>
			<xsd:element ref="gin:TSGReservedIndicator"/>
			<xsd:element ref="gin:TSGDescriptionText"/>
			<xsd:element ref="gin:TSGMethodText"/>
			<xsd:element ref="gin:TraditionalCharacterKeyElementText"/>
			<xsd:element ref="gin:SpecificCharacterKeyElementText"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:element name="TSGDescriptionText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Description of the product, with an indication of its main properties, for a TSG claim</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="TSGMethodText" type="com:LocalizedTextType">
		<xsd:annotation>
			<xsd:documentation>Description of the production method to be applied by producers, for a TSG claim</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:element name="TSGReservedIndicator" type="xsd:boolean">
		<xsd:annotation>
			<xsd:documentation>Indicates whether the application for registration for a TSG claim is made with or without reservation of the name</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
<xsd:element name="UniquenessText" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Comparison with other similar products to establish its uniqueness.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
	
<xsd:element name="WithdrawalDate" type="com:DateType">
				<xsd:annotation>
					<xsd:documentation>Withdrawal Date </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
</xsd:schema>