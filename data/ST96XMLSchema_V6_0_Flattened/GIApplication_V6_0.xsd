	<xsd:element name="GIGrantProtectionLegalBasis" type="gin:GIGrantProtectionLegalBasisType">
		<xsd:annotation>
			<xsd:documentation>Legal basis for the grant of protection of the Geographic Indication</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	
	<xsd:complexType name="GIGrantProtectionLegalBasisType">
		<xsd:choice>
			<xsd:element ref="gin:GILegalBasisLegislativeProvision"/>
			<xsd:element ref="gin:GILegalBasisJudicialDecision"/>
			<xsd:element ref="gin:GILegalBasisRegistrationOffice"/>
		</xsd:choice>
		<xsd:attribute ref="com:documentFileName"/>
	</xsd:complexType>
	
	<xsd:element name="GILegalBasisJudicialDecision" type="gin:GILegalBasisJudicialDecisionType">
	<xsd:annotation>
		<xsd:documentation>Used to capture the legal basis under which the grant of protection is sought. Optionally includes a copy of the provisions, decision or registration referred to.</xsd:documentation>
	</xsd:annotation>
	</xsd:element>
	
	<xsd:complexType name="GILegalBasisJudicialDecisionType">
		<xsd:sequence>
			<xsd:element ref="com:Contact"/>
			<xsd:element ref="com:DecisionDate"/>
		</xsd:sequence>
		<xsd:attribute ref="com:id"/>
	</xsd:complexType>
	
	<xsd:element name="GILegalBasisLegislativeProvision" type="gin:GILegalBasisLegislativeProvisionType">
	<xsd:annotation>
		<xsd:documentation>Legislative or regulatory provisions which form the legal basis for the grant of protection in the country of origin. The titles and dates must be provided.</xsd:documentation>
	</xsd:annotation>
	</xsd:element>
	
	<xsd:complexType name="GILegalBasisLegislativeProvisionType">
		<xsd:sequence>
			<xsd:element ref="com:LegalProvisionText"/>
			<xsd:element ref="com:DocumentDate"/>
		</xsd:sequence>
		<xsd:attribute ref="com:id"/>
	</xsd:complexType>
	
	<xsd:element name="GILegalBasisRegistrationOffice" type="gin:GILegalBasisRegistrationOfficeType">
	<xsd:annotation>
		<xsd:documentation>National or Registration office which form the legal basis for the grant of protection in the country of origin. The name of the office which the registration was made as well as the date and registration number must be provided.</xsd:documentation>
	</xsd:annotation>
	</xsd:element>
	
	<xsd:complexType name="GILegalBasisRegistrationOfficeType">
		<xsd:sequence>
			<xsd:element ref="com:Contact"/>
			<xsd:element ref="com:ApplicationNumber"/>
			<xsd:element ref="com:ApplicationDate"/>
		</xsd:sequence>
		<xsd:attribute ref="com:id"/>
	</xsd:complexType>
	
